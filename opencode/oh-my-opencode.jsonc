{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "auto_update": false,
  "notification": { "force_enable": true },
  "agents": {
    "sisyphus": {
      "description": "Primary orchestrator agent - The Boss",
      // Switched to Flash for faster loops and better agentic performance
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",

      // "model": "google/antigravity-gemini-3-pro",
      // "model": "google/antigravity-claude-opus-4-5-thinking",

      "prompt_append": "\n---\n\n## Session Management (REQUIRED)\n\nBefore investigating any issue:\n1. Use `session_search` to find relevant prior sessions\n2. Use `session_read` to review prior work\n\nBefore completing:\n1. Ensure your session contains a summary of work done",
    },
    "prometheus": {
      "description": "The Planner - High Reasoning",
      // Kept Pro for "Deep Think" architecture planning
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high",
    },
    "metis": {
      "description": "The Critic - Plan Reviewer",
      // Switched to Flash for rapid code review
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
    "atlas": {
      "description": "The Executor - Manages workers",
      // Switched to Flash for reliable, fast execution
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
    "oracle": {
      "description": "Technical Expert Advisor",
      // Switched to Flash for faster context retrieval
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
    "librarian": {
      "description": "Docs & Context Search",
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
    "explore": {
      "description": "Codebase Navigation",
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
  },
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false,
  },
  "categories": {
    "visual-engineering": {
      "model": "google/antigravity-gemini-3-pro",
    },
    "ultrabrain": {
      "description": "For complex logic & refactoring",
      // Kept Pro for heavy logic tasks
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high",
    },
    "artistry": {
      "description": "For UI/UX tasks (Frontend)",
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high",
    },
    "quick": {
      "description": "Simple tasks & git ops",
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
    "unspecified-low": {
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
    "unspecified-high": {
      // Kept Pro as the fallback for 'High' difficulty
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high",
    },
    "writing": {
      "description": "Documentation & Text generation",
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high",
    },
  },
}
