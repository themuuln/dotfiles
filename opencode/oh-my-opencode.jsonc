{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "auto_update": false,
  "notification": { "force_enable": true },
  "agents": {
    "sisyphus": {
      "description": "Primary orchestrator agent - The Boss (Claude Opus 4.5)",
      "model": "google/antigravity-claude-opus-4-5-thinking",
      "variant": "high",

      // "model": "google/antigravity-gemini-3-pro",
      // "model": "google/antigravity-claude-sonnet-4-5-thinking-thinking",

      "prompt_append": "\n---\n\n## Session Management (REQUIRED)\n\nBefore investigating any issue:\n1. Use `session_search` to find relevant prior sessions\n2. Use `session_read` to review prior work\n\nBefore completing:\n1. Ensure your session contains a summary of work done",
    },
    "prometheus": {
      "description": "The Planner - Project Planning (Claude Opus 4.5)",
      "model": "google/antigravity-claude-opus-4-5-thinking",
      "variant": "high",
    },
    "metis": {
      "description": "The Critic - Plan Reviewer (GPT 5.3 High)",
      "model": "openai/gpt-5.3-codex",
      "variant": "high",
    },
    "atlas": {
      "description": "The Executor - Manages workers",
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
    "oracle": {
      "description": "Technical Expert Advisor - Arch/Debug (GPT 5.3 High)",
      "model": "openai/gpt-5.3-codex",
      "variant": "high",
    },
    "hephaestus": {
      "description": "GPT 5.3 Codex Medium Thinking",
      "model": "openai/gpt-5.3-codex",
      "variant": "medium",
    },
    "librarian": {
      "description": "Docs & Context Search",
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
    "explore": {
      "description": "Codebase Navigation",
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
  },
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false,
  },
  "categories": {
    "visual-engineering": {
      "description": "FE/UI (Gemini 3 Pro High)",
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high",
    },
    "ultrabrain": {
      "description": "Deep Reasoning (GPT 5.3 Codex X-High)",
      "model": "openai/gpt-5.3-codex",
      "variant": "xhigh",
    },
    "artistry": {
      "description": "Creative Tasks (Gemini 3 Pro High)",
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high",
    },
    "quick": {
      "description": "Simple tasks (Claude Haiku 4.5)",
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
    "unspecified-low": {
      "description": "Medium tasks (Claude Sonnet 4.5)",
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high",
    },
    "unspecified-high": {
      "description": "Complex tasks (Claude Opus 4.5)",
      "model": "google/antigravity-claude-sonnet-4-5-thinking",
      "variant": "high",
    },
    "writing": {
      "description": "Documentation & Text generation",
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high",
    },
  },
}
